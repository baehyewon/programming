<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/areaselect"
    tools:context="com.example.inniiis.hangang.MainActivity">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginBottom="57dp"
        android:layout_marginLeft="61dp"
        android:layout_marginStart="61dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <ImageButton
                android:id="@+id/gs"
                android:layout_width="55dp"
                android:layout_height="55dp"
                android:layout_alignBaseline="@+id/ng"
                android:layout_alignBottom="@+id/ng"
                android:layout_toStartOf="@+id/ng"
                android:background="@null"/>

            <ImageButton
                android:id="@+id/ng"
                android:layout_width="55dp"
                android:layout_height="55dp"
                android:layout_marginStart="35dp"
                android:background="@null"/>

            <ImageButton
                android:id="@+id/yh"
                android:layout_width="55dp"
                android:layout_height="55dp"
                android:layout_alignStart="@+id/bp"
                android:layout_below="@+id/ng"
                android:layout_marginBottom="20dp"
                android:layout_marginStart="35dp"
                android:background="@null"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <ImageButton
                android:id="@+id/mw"
                android:layout_width="55dp"
                android:layout_height="55dp"
                android:layout_below="@+id/mw"
                android:background="@null"/>

            <ImageButton
                android:id="@+id/yud"
                android:layout_width="55dp"
                android:layout_height="55dp"
                android:layout_alignBaseline="@+id/ic"
                android:layout_alignBottom="@+id/ic"
                android:layout_alignStart="@+id/ng"
                android:layout_marginStart="35dp"
                android:background="@null"/>

            <ImageButton
                android:id="@+id/ic"
                android:layout_width="55dp"
                android:layout_height="55dp"
                android:layout_alignEnd="@+id/js"
                android:layout_below="@+id/ng"
                android:layout_marginBottom="20dp"
                android:layout_marginStart="35dp"
                android:background="@null"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <ImageButton
                android:id="@+id/bp"
                android:layout_width="55dp"
                android:layout_height="55dp"
                android:layout_alignBaseline="@+id/js"
                android:layout_alignBottom="@+id/js"
                android:layout_toEndOf="@+id/mw"
                android:background="@null"/>

            <ImageButton
                android:id="@+id/jw"
                android:layout_width="55dp"
                android:layout_height="55dp"
                android:layout_alignBaseline="@+id/js"
                android:layout_alignBottom="@+id/js"
                android:layout_marginStart="35dp"
                android:layout_toStartOf="@+id/js"
                android:background="@null"/>

            <ImageButton
                android:id="@+id/ds"
                android:layout_width="55dp"
                android:layout_height="55dp"
                android:layout_marginBottom="20dp"
                android:layout_marginStart="35dp"
                android:layout_toEndOf="@+id/ng"
                android:background="@null"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <ImageButton
                android:id="@+id/js"
                android:layout_width="55dp"
                android:layout_height="55dp"
                android:layout_alignBaseline="@+id/js"
                android:layout_alignBottom="@+id/js"
                android:layout_toEndOf="@+id/mw"
                android:background="@null"/>

            <ImageButton
                android:id="@+id/gnr"
                android:layout_width="55dp"
                android:layout_height="55dp"
                android:layout_alignBaseline="@+id/js"
                android:layout_alignBottom="@+id/js"
                android:layout_marginStart="35dp"
                android:layout_toStartOf="@+id/js"
                android:background="@null"/>

            <ImageButton
                android:id="@+id/cancle"
                android:layout_width="55dp"
                android:layout_height="55dp"
                android:layout_marginBottom="20dp"
                android:layout_marginStart="35dp"
                android:layout_toEndOf="@+id/ng"
                android:background="@null"/>
        </LinearLayout>
    </LinearLayout>
</RelativeLayout>
